<%= yield :additional_js %>
$("<%= selector %>").modal('hide');
$("<%= selector %>").on('hidden.bs.modal', function(e){
  $(e.target).remove();
  $("#flash").append("<%= j render partial: "layouts/shared/flash", locals: {flash: flash}%>");
  $("#flash .alert").each(function(i, e){
    $(this).delay(i*1000).fadeIn('slow');
  });
});
